<!--pages/tax-year/tax-year.wxml-->
<view class="container">
<view>年总收入：{{userSalary*12}}</view>
<view>全年五险一金总和：{{total*12}}</view>
<view>全年应缴税额：{{need}}</view>
<view>全年税后工资：{{userSalary*12-need}}</view>
<view class="table">
  <view class="tr bg-w">
    <view class="th">月份</view>
    <view class="th">收入</view>
    <view class="th">五险一金</view>
    <view class="th ">应缴税额</view>
    <view class="th ">税后工资</view>
  </view>
  <block wx:for="{{taxarray}}" wx:key="{{code}}">
    <view class="tr bg-g" wx:if="{{index != 0}}">
      <view class="td">{{index}}</view>
      <view class="td">{{userSalary}}</view>
      <view class="td">{{total}}</view>
      <view class="td">{{item}}</view>
      <view class="td">{{userSalary-item}}</view>
    </view>
  </block>
</view>
</view>