<!--pages/tax-count/tax-count.wxml-->
<view class='center  font'>
  <!--center-top-->
  <view class='center-top'>
    <view class='salary-mouth'>{{title}}薪资清单</view>
    <view class='get'>到手工资</view>
    <view class='get2'>
      <view class='get-salary'>{{sum}}</view>
      <view class='yuan'>元</view>
    </view>
  </view>
  <!--center-bottom-->
  <view class='center-bottom'>
    <view class="weui-cell">
      <view class="weui-cell__bd">养老保险金</view>
      <view class="weui-cell__ft">{{oldFee}}元（8%)</view>
      </view>
      <view class="weui-cell">
      <view class="weui-cell__bd">医疗保险金</view>
      <view class="weui-cell__ft"> {{medicalFee}}元（{{medicalRate}}%)</view>
      </view>
      <view class="weui-cell">
      <view class="weui-cell__bd">失业保险金</view>
      <view class="weui-cell__ft">{{jobFee}}元({{jobRate}}%)</view>
      </view>
      <view class="weui-cell">
      <view class="weui-cell__bd">基本住房公积金</view>
      <view class="weui-cell__ft">{{houseFee}}元（{{commonRate}}%)</view>
      </view>
      <view class="weui-cell">
      <view class="weui-cell__bd">五险一金总计</view>
      <view class="weui-cell__ft">{{total}}元</view>
      </view>
      <view class="weui-cell">
      <view class="weui-cell__bd">专项附加扣除</view>
      <view class="weui-cell__ft">{{specialFee}}元</view>
      </view>
      <view class="weui-cell">
      <view class="weui-cell__bd">其他扣除</view>
      <view class="weui-cell__ft">{{otherFee}}元</view>
      </view>
      <view class="weui-cell">
      <view class="weui-cell__bd">应税工资</view>
      <view class="weui-cell__ft">{{shouldTax}}元</view>
      </view>
      <view class="weui-cell">
      <view class="weui-cell__bd">个人所得税</view>
      <view class="weui-cell__ft">{{taxarray[month]}}元</view>
      </view>
      </view>
      <canvas canvas-id="pieCanvas" class="canvas" style="height:300px" bindtouchstart="touchHandler"></canvas>
     </view>

